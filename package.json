{
  "name": "school-website-builder",
  "version": "1.0.0",
  "description": "A modern, open-source platform for building beautiful school websites with integrated academic management features",
  "keywords": [
    "school",
    "education",
    "website",
    "nextjs",
    "typescript",
    "tailwindcss",
    "academic",
    "student-management",
    "opensource"
